{
  "name": "towa-theme",
  "version": "4.1.0",
  "author": "TOWA GMBH",
  "private": true,
  "scripts": {
    "create-component": "node ./tools/create-component.js",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "run-s lint:all:*",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue resources",
    "lint:all:stylelint": "yarn lint:stylelint \"resources/**/*.{vue,scss}\"",
    "lint:all:prettier": "yarn lint:prettier \"resources/**/*.{js,json,css,scss,vue,md}\"",
    "preinstall": "$npm_execpath --silent run checkyarn",
    "checkyarn": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn for installing: https://yarnpkg.com/en/docs/install')\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chalk": "^2.1.0",
    "cross-env": "^5.1.4",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "fs": "^0.0.1-security",
    "husky": "^1.0.0",
    "laravel-mix": "^5.0.1",
    "lint-staged": "^8.0.3",
    "markdownlint-cli": "^0.13.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5"
  },
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "axios": "^0.18.0",
    "body-scroll-lock": "^2.6.4",
    "commitlint": "^8.3.5",
    "core-js": "^3.6.4",
    "dotenv": "^7.0.0",
    "imagemin-lint-staged": "^0.4.0",
    "include-media": "^1.4.9",
    "jquery": "^3.2.1",
    "normalize.css": "^8.0.1",
    "regenerator-runtime": "^0.13.3",
    "slick-carousel": "^1.6.0",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^3.1.3",
    "vue-multiselect": "^2.1.0"
  },
  "engines": {
    "node": ">=11.0.0",
    "yarn": ">=1.21.0"
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11",
    "not OperaMobile <= 46",
    "not BlackBerry <= 10",
    "not Baidu <= 8",
    "not kaios <= 3",
    "not QQAndroid < 2",
    "not IE_Mob <= 11"
  ]
}
